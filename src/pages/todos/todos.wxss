.header {
  display: flex;
  align-items: center;
  justify-content: flex-end; /* 让图标尽可能靠右排列 */
  border-radius: 10rpx;
  margin-bottom: 30rpx;
  padding: 20rpx;
  width: 94%;
  height: 90rpx;
  /* background-color: #ffffff; */
  text-align: center;
  margin-left: 3%;
}

/* 新增日期样式 */
.date {
  margin-right: auto; /* 将日期元素向左移动，与图标之间留出空白 */
}


.back_container {
  height: 100%;
  background-color: #ededed;
}
.todo_container{height: 80%;
  background-color: #ededed;
}

.header .plus {
  width: 50rpx;
  height: 50rpx;
  margin-right: 10rpx;
}

.header .new-todo {
  flex: 1;
  font-size: 28rpx;
  margin-left: 5%;
  margin-right: 5%
}


.todos {
  /* border: 3rpx solid #e0e0e0; */
  border-radius: 10rpx;
  /* box-shadow: 0 0 5rpx #e0e0e0; */
  margin-top: 10rpx;
  width: 94%;
  margin-left: 3%;
  /* background-color: #f5f5f5; */
  }
  

.todos .item {
  display: flex;
  height: 120rpx;
  align-items: center;
  padding: 25rpx;
  border-bottom: 1rpx solid #e0e0e0;
  
  background-color: #6f879f;
  border-radius: 10rpx;
  margin-bottom: 10rpx;
  background-image: url("https://cdn.mathpix.com/snip/images/b2Cb0jqRYktyyeOwdjHGZmLCJNhFsyJ2zbr6cGgyFZs.original.fullsize.png");
  background-size: 110%;
  background-repeat: no-repeat;
}
