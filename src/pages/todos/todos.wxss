.todos .item:last-child {
  border-bottom: 0;
}

.todos .item .checkbox {
  margin-right: 20rpx;
}

.todos .item .name {
  flex: 1;
  font-size: 30rpx;
  /* color: #444; */
  color: #ffffff;
}
.todos .item.completed {
  background-color:#d5d5d5;
  background-image: none;
}
.todos .item.completed .name {
  text-decoration: line-through;
  color: #888;
}
